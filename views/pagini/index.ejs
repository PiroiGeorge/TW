      <script>
        function setSubtitle(limba) {
          const video = document.getElementById("video-subtitrari");
          const tracks = video.textTracks;
          for (let i = 0; i < tracks.length; i++) {
            tracks[i].mode = (tracks[i].language === limba) ? "showing" : "disabled";
          }
      
          if (limba === null) {
            for (let track of tracks) {
              track.mode = "disabled";
            }
          }
        }
