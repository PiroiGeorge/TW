            <%
        function oraToMinute(ora) {
          let [h, m] = ora.split(":").map(e => parseInt(e));
          return h * 60 + m;
        }
        
        function inInterval(oraCurenta, interval) {
          let [start, end] = interval.split("-");
          return oraToMinute(start) <= oraCurenta && oraCurenta <= oraToMinute(end);
        }
        
        let d = new Date();
        let oraCurenta = d.getHours() * 60 + d.getMinutes();
        let vImaginiFiltrate = [];
        
        for (let imag of locals.imagini) {
          if (imag.timp && inInterval(oraCurenta, imag.timp)) {
            vImaginiFiltrate.push(imag);
          }
        }
        vImaginiFiltrate = vImaginiFiltrate.slice(0,10); // max 10 imagini
        
        for (let imag of vImaginiFiltrate) {
        %>
            <figure title="<%- imag.descriere %>">
              <picture>
                <source srcset="<%- imag.fisier_mediu %>" media="(max-width:1000px)">
                <img src="<%- imag.fisier %>" alt="<%- imag.titlu ? imag.titlu : imag.descriere %>">
              </picture>
              <figcaption><%- imag.titlu %></figcaption>
            </figure>
        <% } %>
